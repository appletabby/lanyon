<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>var numberArray = [60, 50, 62, 58, 54, 54];

function compareNumbers(num1, num2) {
  if (num1 &gt; num2) {
    return 1;
  } else if (num1 === num2) {
    return 0;
  } else {
    return -1;
  }
}

numberArray.sort(compareNumbers);
console.log(numberArray);
// [50, 54, 54, 58, 60, 62]
